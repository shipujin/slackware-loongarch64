-        if ( e.hasAttribute( "intent" ) )
-            setLineIntent((LineAnnotation::LineIntent)e.attribute( "intent" ).toInt());
-
-        // parse all 'point' subnodes
-        QLinkedList<QPointF> points;
-        QDomNode pointNode = e.firstChild();
-        while ( pointNode.isElement() )
-        {
