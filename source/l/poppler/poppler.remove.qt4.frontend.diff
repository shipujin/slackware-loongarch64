-            setLineStartStyle((LineAnnotation::TermStyle)e.attribute( "startStyle" ).toInt());
-        if ( e.hasAttribute( "endStyle" ) )
-            setLineEndStyle((LineAnnotation::TermStyle)e.attribute( "endStyle" ).toInt());
