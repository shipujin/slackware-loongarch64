-void PdfViewer::slotOpenFile()
-{
-    QString fileName = QFileDialog::getOpenFileName(this, tr("Open PDF Document"), QDir::homePath(), tr("PDF Documents (*.pdf)"));
-    if (fileName.isEmpty()) {
-        return;
-    }
-
-    loadDocument(fileName);
